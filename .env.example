# OpenAI
OPENAI_API_KEY=sk-...
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
OPENAI_CONVERSATION_MODEL=gpt-5.2
OPENAI_ANALYTICS_MODEL=gpt-5-mini

# Redis (must be redis-stack for vector search)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Tavily Search
TAVILY_API_KEY=tvly-...

# Agent behaviour
SIMILARITY_THRESHOLD=0.7
CHUNK_SIZE=500
CHUNK_OVERLAP=50
MAX_SEARCH_RESULTS=5
MAX_PAGES_TO_FETCH=3

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/agent.log
ANALYTICS_FILE=logs/analytics.jsonl
